      <div class="col s12 m5">
        <div class="card blue-grey darken-1">
          <div class="card-content white-text">
            <span class="card-title">Delete Sticky Note - Single</span>
            <p>URL: <span class="urlBinding">deleteStickyNote</span> </p>
            <p>Method: POST</p>
            <p>Sample Payload:</p>
            <p>
              <pre>
    {
        "id": 1
    }
    </pre>
            </p>
            <p>Sample Response:</p>
            <p>
              <pre>
    {
        "result": "Success"
    }
    </pre>
            </p>
          </div>
          <div class="card-action">

          </div>
        </div>
      </div>

      <div class="col s12 m5">
        <div class="card blue-grey darken-1">
          <div class="card-content white-text">
            <span class="card-title">Delete Sticky Note - Multiple</span>
            <p>URL: <span class="urlBinding">deleteStickyNote</span></p>
            <p>Method: POST</p>
            <p>Sample Payload:</p>
            <p>
              <pre>
      {
          "id": [1,2,3]
      }
      </pre>
            </p>
            <p>Sample Response:</p>
            <p>
              <pre>
      {
          "result": "Success"
      }
      </pre>
            </p>
          </div>
          <div class="card-action">

          </div>
        </div>
      </div>

    </div>
